# Copy this file to .env and fill in your actual credentials

# Twilio Configuration
REACT_APP_TWILIO_ACCOUNT_SID=your_twilio_account_sid
REACT_APP_TWILIO_AUTH_TOKEN=your_twilio_auth_token
REACT_APP_TWILIO_FROM_NUMBER=your_twilio_phone_number
REACT_APP_TWILIO_TO_NUMBER=recipient_phone_number

# RapidAPI Configuration
REACT_APP_RAPIDAPI_KEY=your_rapidapi_key
REACT_APP_RAPIDAPI_WEATHER_HOST=open-weather13.p.rapidapi.com
REACT_APP_RAPIDAPI_CITIES_HOST=country-state-city-search-rest-api.p.rapidapi.com

# Default Country Code for Cities (e.g., IN for India, US for United States)
REACT_APP_DEFAULT_COUNTRY_CODE=IN